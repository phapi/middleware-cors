{
  "name": "phapi/middleware-cors",
  "description": "Middleware handling HTTP access control (CORS) requests",
  "keywords": [
    "middleware", "HTTP access control", "cors", "phapi", "access", "control"
  ],
  "version": "1.0.0",
  "minimum-stability": "stable",
  "license": "MIT",
  "homepage": "http://phapi.github.io",
  "authors": [
    {
      "name": "Peter Ahinko",
      "email": "peter@ahinko.se",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "phapi/contract": "1.*",
    "phapi/exception": "1.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "mockery/mockery": "0.9.*",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Phapi\\Middleware\\Cors\\": "src/Phapi/Middleware/Cors/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Phapi\\Tests\\Middleware\\Cors\\": "tests/Phapi/Middleware/Cors/"
    }
  }
}
